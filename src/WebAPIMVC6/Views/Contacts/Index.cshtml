<div class="row">
     <div class="col-md-12">
          <h4>Paged Contact List</h4>
          <br />
     </div>
</div>
<div class="row">
     <div class="col-md-12">
          <div style="max-height: 1200px; overflow-y: auto">
               <table id="ContactsTable" ctTotalItems="100" class="table table-striped table-bordered table-hover table-condensed">
                    <thead>
                         <tr id="colHeaders">
                              <th id="LastName" class="sortable"><a href="#">Last Name</a></th>
                              <th id="FirstName" class="sortable"><a href="#">First Name</a></th>
                              <th id="City.Name" class="sortable"><a href="#">City</a></th>
                              <th id="City.State.Name" class="sortable"><a href="#">State</a></th>
                              <th><b>Active</b></th>
                         </tr>
                    </thead>
                    <tbody id="ContactsList"></tbody>
               </table>
          </div>
     </div>
</div>
<script id="contactsListHGrid" type="text/x-handlebars-template">
     {{#ContactsList}}
     <tr>
          <td>{{lastName}}</td>
          <td>{{firstName}}</td>
          <td>{{city.name}}</td>
          <td>{{city.state.name}}</td>
          <td>{{active}}</td>
     </tr>
     {{/ContactsList}}
</script>
@section scripts {
     <script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.0.5/handlebars.min.js" type="text/javascript"></script>
     <script src="~/js/ajaxcommon.js" type="text/javascript"></script>
     <script src="~/js/turbotables.js" type="text/javascript"></script>
     <script src="~/js/pagedcontacts.js" type="text/javascript"></script>
}
