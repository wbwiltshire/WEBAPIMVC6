<div class="row">
     <div class="col-md-12">
          <h4>Contact List</h4>
          <br />
     </div>
</div>
<div class="row">
     <div class="col-md-12">
          <div style="max-height: 1200px; overflow-y: auto">
               <table id="ContactsTable" class="table table-striped table-bordered table-hover table-condensed">
                    <thead>
                         <tr id="colHeaders">
                              <th><b>Last Name</b></th>
                              <th><b>First Name</b></th>
                              <th><b>City</b></th>
                              <th><b>State</b></th>
                              <th><b>Active</b></th>
                         </tr>
                    </thead>
                    <tbody id="ContactsList"></tbody>
               </table>
          </div>
     </div>
</div>
<script id="contactsListHGrid" type="text/x-handlebars-template">
     {{#ContactsList}}
     <tr>
          <td>{{lastName}}</td>
          <td>{{firstName}}</td>
          <td>{{city.name}}</td>
          <td>{{city.state.name}}</td>
          <td>{{active}}</td>
     </tr>
     {{/ContactsList}}
</script>
@section scripts {
     <script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.0.5/handlebars.min.js" type="text/javascript"></script>
     <script src="/js/ajaxcommon.js" type="text/javascript"></script>
     <script src="~/js/listcontacts.js" type="text/javascript"></script>
}
